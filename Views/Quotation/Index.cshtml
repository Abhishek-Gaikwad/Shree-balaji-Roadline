@model IEnumerable<Trans9.Models.Quotes>
@section Styles{
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" />

    <style>
        .table {
            font-size: 11px;
        }

        .btn-sm {
            padding: .25rem .5rem;
            font-size: .875rem;
            line-height: 1.2;
            border-radius: 0.2rem;
        }
    </style>
}
<div class="row">
    <div class="col-md-10">
        <h4> @ViewBag.PageTitle</h4>
    </div>
    <div class="col-md-2 p-2 text-right">
        <a onclick="showInPopup('@Url.Action("AddQuote","Quotation",null,Context.Request.Scheme)', 'New Quotation')" class="btn btn-success btn-sm"><span class="fa fa-plus"></span> Add New Quotation</a>
    </div>
</div>
<div id="view-all-transaction">
    @await Html.PartialAsync("_ViewAllQuotes", Model)
</div>
    @section Scripts {
    <script src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js" type="text/javascript"></script>
    <!-- Latest compiled and minified JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>
    <script src="~/js/quote.js"></script>
}
