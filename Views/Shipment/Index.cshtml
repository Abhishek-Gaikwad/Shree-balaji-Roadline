@model IEnumerable<Trans9.Models.Shipment>
@section Styles{
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" />

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" />
    <link type="text/css" href="//gyrocode.github.io/jquery-datatables-checkboxes/1.2.12/css/dataTables.checkboxes.css" rel="stylesheet" />
    }
<div class="row">
    <div class="col-md-8">
        <h4> @ViewBag.PageTitle</h4>
    </div>
    <div class="col-md-4 p-2 text-right">
        <a onclick="showInPopup('@Url.Action("AddBulk","Shipment",null,Context.Request.Scheme)', 'Bulk Shipment')" class="btn btn-success btn-sm mr-2"><span class="fa fa-plus"></span> Add Bulk Shipments</a>
        <a onclick="showInPopup('@Url.Action("AddOrEdit","Shipment",null,Context.Request.Scheme)', 'New Shipment')" class="btn btn-success btn-sm"><span class="fa fa-plus"></span> Add New Shipment</a>
    </div>
</div>
<div id="view-all-transaction">
    @await Html.PartialAsync("_ViewAllShipments", Model)
</div>
<div id="dvjson"></div>
@section Scripts {
    <script src="../../dist/datatable/js/jquery.datatables.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="//gyrocode.github.io/jquery-datatables-checkboxes/1.2.12/js/dataTables.checkboxes.min.js"></script>
    <!-- Latest compiled and minified JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>
    <script src="../../js/excelexportjs.js"></script>
    <script src="../../js/app.js"></script>
}