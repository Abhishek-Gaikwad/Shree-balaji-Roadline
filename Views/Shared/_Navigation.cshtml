                                <a href="/@item.urlName" class="nav-link @item.active">
                                    <i class="@item.icon nav-icon"></i>
                                    <p>@item.pageName</p>
                                </a>
                            </li>
                        }

                    </ul>
                </li>
            }
            @if (permit.Any())
            {
                permitActive = permit.Where(x => !string.IsNullOrEmpty(x.active)).Any();
                <li class="nav-item @(permitActive? "menu-open":"")">
                    <a href="#" class="nav-link @(permitActive? "active":"")">
                        <i class="nav-icon fas fa-notes-medical"></i>
                        <p>
                            USER &amp; ROLES
                            <i class="right fas fa-angle-left"></i>
                        </p>
                    </a>
                    <ul class="nav nav-treeview">
                        @foreach (var item in permit)
                        {
                            <li class="nav-item">
                                <a href="/@item.urlName" class="nav-link @item.active">
                                    <i class="@item.icon nav-icon"></i>
                                    <p>@item.pageName</p>
                                </a>
                            </li>
                        }

                    </ul>
                </li>
            }
        </ul>
    </nav>
    <!-- /.sidebar-menu -->
</div>