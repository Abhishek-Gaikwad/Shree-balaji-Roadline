@model IEnumerable<Trans9.Models.Shipment>
<div class="card">
    <div class="table-content">
        <div class="card-body table-responsive p-0" style="height: 450px;">
            <table class="table table-head-fixed text-nowrap table-bordered table-hover">
                <thead class="header">
                    <tr>
                        <th>
                            #
                        </th>
                        <th>ACTION</th>
                        <th>
                            @Html.DisplayNameFor(model => model.shipmentNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.shipmentDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.dealerId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.detsinationId)
                        </th>
                        @*<th>
                        @Html.DisplayNameFor(model => model.location)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.trasitDays)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.region)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.routeCode)
                        </th>*@
                        <th>
                            @Html.DisplayNameFor(model => model.vcNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.modelDesc)
                        </th>
                        @* <th>
                        @Html.DisplayNameFor(model => model.mfgCode)
                        </th>*@
                        <th>
                            @Html.DisplayNameFor(model => model.plantCode)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.plantDesc)
                        </th>
                        @*   <th>
                        @Html.DisplayNameFor(model => model.mfgRoute)
                        </th>*@
                        <th>
                            @Html.DisplayNameFor(model => model.chasisNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.invoiceNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.invoiceDate)
                        </th>
                        @*  <th>
                        @Html.DisplayNameFor(model => model.freightValue)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.epodNo)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.epodDate)
                        </th>*@
                        <th>
                            @Html.DisplayNameFor(model => model.status)
                        </th>                       
                    </tr>
                </thead>
                <tbody>
                    @if (Model.Any())
                    {
                        int i = 1;
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @(i++)
                                </td>
                                <td>
                                    <a onclick="showInPopup('@Url.Action("AddOrEdit","PlantOut",new { id=item.shipmentId},Context.Request.Scheme)', 'PlantOut')" class="text-primary"><span class="fa fa-pencil-alt"></span></a>
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.shipmentNo)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.shipmentDate)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.dealerName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.dest)
                                </td>
                                @* <td>
                        @Html.DisplayFor(modelItem => item.location)
                        </td>
                        <td>
                        @Html.DisplayFor(modelItem => item.trasitDays)
                        </td>
                        <td>
                        @Html.DisplayFor(modelItem => item.region)
                        </td>
                        <td>
                        @Html.DisplayFor(modelItem => item.routeCode)
                        </td>*@
                                <td>
                                    @Html.DisplayFor(modelItem => item.vcNo)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.modelDesc)
                                </td>
                                @*  <td>
                        @Html.DisplayFor(modelItem => item.mfgCode)
                        </td>*@
                                <td>
                                    @Html.DisplayFor(modelItem => item.plantCode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.plantDesc)
                                </td>
                                @* <td>
                        @Html.DisplayFor(modelItem => item.mfgRoute)
                        </td>*@
                                <td>
                                    @Html.DisplayFor(modelItem => item.chasisNo)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.invoiceNo)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.invoiceDate)
                                </td>
                                @*  <td>
                        @Html.DisplayFor(modelItem => item.freightValue)
                        </td>
                        <td>
                        @Html.DisplayFor(modelItem => item.epodNo)
                        </td>
                        <td>
                        @Html.DisplayFor(modelItem => item.epodDate)
                        </td>*@
                                <td>
                                    @Html.DisplayFor(modelItem => item.status)
                                </td>                               
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
